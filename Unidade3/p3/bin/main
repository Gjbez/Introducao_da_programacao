import java.util.Scanner

public class Main{
    public Main() {
        Scanner o = new Scanner(System.in);
        int a = o.nextInt();
        int b = o.nextInt();
        int x = a + b;

        System.out.println("X = " + x);

        o.close();
    }
    public static void main(String[] args){
        new Main();
    }
}